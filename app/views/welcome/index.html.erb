<h1>Welcome#index</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email%>
  <%= f.email_field :email%><br>
  <%=f.label :password%>
  <%= f.password_field :password%><br>
  <%=f.submit "login"%>
<%end%>

or sign up! 

<%= form_for(:user, url: users_path) do |f| %>
  <%=f.label :email%>
  <%=f.email_field :email%><br>
  <%= f.label :username%>
  <%= f.text_field :username%><br>
  <%=f.label :password%> 
  <%=f.password_field :password%><br>
  <%=f.submit "sign up"%>
<%end%>


